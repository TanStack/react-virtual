{
  "name": "@tanstack/solid-virtual",
  "version": "3.1.0",
  "description": "Headless UI for virtualizing scrollable elements in Solid",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanstack/virtual.git"
  },
  "homepage": "https://github.com/tanstack/virtual#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./coverage",
    "test:types": "tsc",
    "test:build": "publint --strict",
    "build": "vite build"
  },
  "keywords": [
    "react",
    "vue",
    "solid",
    "virtual",
    "virtual-core",
    "datagrid"
  ],
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@tanstack/virtual-core": "workspace:*"
  },
  "devDependencies": {
    "solid-js": "^1.3.15",
    "vite-plugin-solid": "^2.10.1"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0"
  }
}
